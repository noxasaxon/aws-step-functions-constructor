{
  "name": "aws-step-functions-constructor",
  "displayName": "AWS step functions constructor",
  "description": "Visualization of AWS step functions",
  "icon": "images/newPic.png",
  "version": "0.8.1",
  "publisher": "paulshestakov",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulShestakov/aws-step-functions-constructor.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:extension.showStepFunction",
    "onCommand:extension.showSoclessWithAPB"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.showStepFunction",
        "title": "Show step function"
      },
      {
        "command": "extension.showSoclessWithAPB",
        "title": "Visualize SOCless CloudFormation after APB"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/dagre-d3": "^0.6.2",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.162",
    "@types/node": "^8.10.25",
    "@types/ramda": "^0.27.34",
    "@types/uuid": "^8.3.0",
    "prettier": "^2.1.2",
    "ts-loader": "^8.0.7",
    "tslint": "^5.8.0",
    "typescript": "^4.0.5",
    "vscode": "^1.1.37",
    "webpack": "^5.3.1",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "dagre": "^0.8.5",
    "dagre-d3": "^0.6.4",
    "js-yaml": "^3.13.1",
    "json-source-map": "^0.6.1",
    "lodash": "^4.17.20",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  }
}
